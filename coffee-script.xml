<templateSet group="CoffeeScript">
  <template name="con" value="console.log '$TEXT$'" description="console.log" toReformat="false" toShortenFQNames="true">
    <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CoffeeScript" value="true" />
    </context>
  </template>
  <template name="fun" value="$NAME$: ($PARAM$) -&gt;&#10;&#9;$END$" description="function" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CoffeeScript" value="true" />
    </context>
  </template>
  <template name="funf" value="$NAME$: ($PARAM$) =&gt;&#10;&#9;$END$" description="function with fat arrow" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CoffeeScript" value="true" />
    </context>
  </template>
  <template name="jq" value="$('$NAME$').$END$" description="jQuery selector" toReformat="false" toShortenFQNames="true">
